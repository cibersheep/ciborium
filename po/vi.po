msgstr "Hành động này sẽ xoá toàn bộ dữ liệu trên thiết bị"

#: share/ciborium/qml/components/FormatDialog.qml:65
msgid "Formatting"
msgstr "Đang định dạng"

#: share/ciborium/qml/components/FormatDialog.qml:87
msgid "Format Complete"
msgstr "Định dạng hoàn tất"

#: share/ciborium/qml/components/FormatDialog.qml:110
msgid "Format Error"
msgstr "Định dạng lỗi"

